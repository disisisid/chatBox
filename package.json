{
  "private": true,
  "name": "chatBox",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && sirv build --cors --single",
    "deploy": "gh-pages -d build",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-synacor": "^3.0.4",
    "if-env": "^1.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2",
    "preact-router": "^3.2.1",
    "socket.io-client": "^2.3.0"
  }
}
